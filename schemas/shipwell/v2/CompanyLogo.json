{
  "description": "The set of logos associated with a company. Use the image_type param to filter by type",
  "properties": {
    "company_name": {
      "type": "string"
    },
    "id": {
      "format": "uuid",
      "type": "string"
    },
    "image_type": {
      "enum": ["LOGO_COLOR", "LOGO_WHITE", "INLINE_COLOR", "INLINE_WHITE"],
      "type": "string"
    },
    "logo": {
      "type": "string"
    }
  },
  "required": ["logo"],
  "type": "object",
  "default": {
    "company_name": "string",
    "id": "3fa85f64-5717-4562-b3fc-2c963f66afa6",
    "image_type": "LOGO_COLOR",
    "logo": "string"
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
