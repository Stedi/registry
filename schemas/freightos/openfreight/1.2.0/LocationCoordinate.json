{
  "description": "Location Coordinates in Latitude and Longitude",
  "required": ["coordinateSystemCode"],
  "type": "object",
  "properties": {
    "coordinateSystemCode": {
      "description": "Coordinate System Code",
      "type": "object",
      "properties": {
        "value": {
          "type": "string"
        },
        "listID": {
          "description": "The identification of a list of codes",
          "type": "string"
        },
        "listAgencyID": {
          "description": "An agency that maintains one or more code lists",
          "type": "string"
        },
        "listAgencyName": {
          "description": "The name of the agency that maintains the code list",
          "type": "string"
        },
        "listName": {
          "description": "The name of a list of codes",
          "type": "string"
        },
        "listVersionID": {
          "description": "The version of the code list",
          "type": "string"
        },
        "name": {
          "description": "The textual equivalent of the code content",
          "type": "string"
        },
        "languageID": {
          "description": "The identifier of the language used in the corresponding text string",
          "type": "string"
        },
        "listURI": {
          "description": "The Uniform Resource Identifier that identifies where the code list is located",
          "type": "string"
        },
        "listSchemeURI": {
          "description": "The Uniform Resource Identifier that identifies where the code list scheme is located",
          "type": "string"
        }
      }
    },
    "latitudeDegreesMeasure": {
      "description": "Latitude (Degrees)",
      "type": "object",
      "properties": {
        "value": {
          "format": "double",
          "description": "Value of the measure",
          "type": "number"
        },
        "unitCode": {
          "description": "The unit type of the measure value (e.g kg, cm)",
          "type": "string"
        }
      }
    },
    "latitudeMinutesMeasure": {
      "description": "Latitude (Minutes)",
      "type": "object",
      "properties": {
        "value": {
          "format": "double",
          "description": "Value of the measure",
          "type": "number"
        },
        "unitCode": {
          "description": "The unit type of the measure value (e.g kg, cm)",
          "type": "string"
        }
      }
    },
    "longitudeDegreesMeasure": {
      "description": "Longitude (Degrees)",
      "type": "object",
      "properties": {
        "value": {
          "format": "double",
          "description": "Value of the measure",
          "type": "number"
        },
        "unitCode": {
          "description": "The unit type of the measure value (e.g kg, cm)",
          "type": "string"
        }
      }
    },
    "longitudeMinutesMeasure": {
      "description": "Longitude (Minutes)",
      "type": "object",
      "properties": {
        "value": {
          "format": "double",
          "description": "Value of the measure",
          "type": "number"
        },
        "unitCode": {
          "description": "The unit type of the measure value (e.g kg, cm)",
          "type": "string"
        }
      }
    }
  },
  "default": {
    "coordinateSystemCode": {
      "value": "string",
      "listID": "string",
      "listAgencyID": "string",
      "listAgencyName": "string",
      "listName": "string",
      "listVersionID": "string",
      "name": "string",
      "languageID": "string",
      "listURI": "string",
      "listSchemeURI": "string"
    },
    "latitudeDegreesMeasure": {
      "value": 0,
      "unitCode": "string"
    },
    "latitudeMinutesMeasure": {
      "value": 0,
      "unitCode": "string"
    },
    "longitudeDegreesMeasure": {
      "value": 0,
      "unitCode": "string"
    },
    "longitudeMinutesMeasure": {
      "value": 0,
      "unitCode": "string"
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
