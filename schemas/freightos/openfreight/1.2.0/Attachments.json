{
  "required": ["attachment"],
  "type": "object",
  "properties": {
    "attachment": {
      "type": "array",
      "items": {
        "required": ["url"],
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "attachmentDateTime": {
            "format": "date-time",
            "type": "string"
          }
        }
      }
    }
  },
  "default": {
    "attachment": [
      {
        "name": "string",
        "url": "string",
        "attachmentDateTime": "1970-01-01T00:00:00.000Z"
      }
    ]
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
