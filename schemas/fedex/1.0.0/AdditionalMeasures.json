{
  "type": "object",
  "properties": {
    "quantity": {
      "type": "number",
      "description": "Specify commodity quantity.",
      "format": "double",
      "example": 12.45
    },
    "units": {
      "type": "string",
      "description": "Unit of measure used to express the quantity of this commodity line item.",
      "example": "KG"
    }
  },
  "default": {
    "quantity": 12.45,
    "units": "KG"
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
