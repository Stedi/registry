        "CONSIGNEE",
        "CUSTOMS",
        "DESTINATION",
        "DESTINATION_CONTROL_STATEMENT",
        "FREIGHT_REFERENCE",
        "MANIFEST",
        "ORIGIN",
        "RECIPIENT",
        "SECOND_ADDRESS",
        "SHIPPER"
      ]
    },
    "count": {
      "type": "integer",
      "description": "Specifies the count of label to return.",
      "example": 1
    }
  },
  "default": {
    "type": "CONSIGNEE",
    "count": 1
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
