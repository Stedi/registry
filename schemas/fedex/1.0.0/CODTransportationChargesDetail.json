{
  "type": "object",
  "properties": {
    "rateType": {
      "type": "string",
      "description": "Specify the Rate Type used.",
      "enum": ["ACCOUNT", "CURRENT", "CUSTOM"]
    },
    "rateLevelType": {
      "type": "string",
      "description": "Specify which level the rate to be applied.",
      "enum": ["BUNDLED_RATE", "INDIVIDUAL_PACKAGE_RATE"]
    },
    "chargeLevelType": {
      "type": "string",
      "description": "Specify which level the charges to be applied.",
      "enum": ["CURRENT_PACKAGE", "SUM_OF_PACKAGES"]
    },
    "chargeType": {
      "type": "string",
      "description": "Specify Charge type.",
      "enum": ["COD_SURCHARGE", "NET_CHARGE", "NET_FREIGHT", "TOTAL_CUSTOMER_CHARGE"]
    }
  },
  "description": "Use this object to specify C.O.D. transportation charges.",
  "default": {
    "rateType": "ACCOUNT",
    "rateLevelType": "BUNDLED_RATE",
    "chargeLevelType": "CURRENT_PACKAGE",
    "chargeType": "COD_SURCHARGE"
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
