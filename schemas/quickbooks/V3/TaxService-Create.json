{
  "description": "Use TaxService to create taxcode and corresponding taxrates",
  "type": "object",
  "title": "TaxService-Create",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "type": "object",
    "properties": {
      "TaxCode": {
        "type": "string"
      },
      "TaxRateDetails": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "TaxRateName": {
              "type": "string"
            },
            "RateValue": {
              "type": "string",
              "format": "utc-millisec"
            },
            "TaxAgencyId": {
              "type": "string",
              "format": "utc-millisec"
            },
            "TaxApplicableOn": {
              "type": "string"
            }
          }
        }
      }
    }
  },
  "default": {
    "TaxCode": "MyTaxCodeName",
    "TaxRateDetails": [
      {
        "TaxRateName": "myNewTaxRateName",
        "RateValue": "8",
        "TaxAgencyId": "1",
        "TaxApplicableOn": "Sales"
      }
    ]
  }
}
