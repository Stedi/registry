{
  "type": "object",
  "properties": {
    "AttachmentID": {
      "description": "Unique ID for the file",
      "type": "string",
      "format": "uuid",
      "examples": ["00000000-0000-0000-0000-000000000000"]
    },
    "FileName": {
      "description": "Name of the file",
      "type": "string",
      "examples": ["xero-dev.jpg"]
    },
    "Url": {
      "description": "URL to the file on xero.com",
      "type": "string",
      "examples": [
        "https://api.xero.com/api.xro/2.0/Accounts/da962997-a8bd-4dff-9616-01cdc199283f/Attachments/sample5.jpg"
      ]
    },
    "MimeType": {
      "description": "Type of file",
      "type": "string",
      "examples": ["image/jpg"]
    },
    "ContentLength": {
      "description": "Length of the file content",
      "type": "integer"
    },
    "IncludeOnline": {
      "description": "Include the file with the online invoice",
      "type": "boolean"
    }
  },
  "default": {
    "AttachmentID": "00000000-0000-0000-0000-000000000000",
    "FileName": "xero-dev.jpg",
    "Url": "https://api.xero.com/api.xro/2.0/Accounts/da962997-a8bd-4dff-9616-01cdc199283f/Attachments/sample5.jpg",
    "MimeType": "image/jpg",
    "ContentLength": 1,
    "IncludeOnline": true
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
