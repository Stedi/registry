{
  "properties": {
    "EmployeeID": {
      "description": "The Xero identifier for an employee e.g. 297c2dc5-cc47-4afd-8ec8-74990b8761e9",
      "type": "string",
      "format": "uuid"
    },
    "Status": {
      "description": "Current status of an employee â€“ see contact status types",
      "type": "string",
      "enum": ["ACTIVE", "ARCHIVED", "GDPRREQUEST", "DELETED"]
    },
    "FirstName": {
      "description": "First name of an employee (max length = 255)",
      "maxLength": 255,
      "type": "string"
    },
    "LastName": {
      "description": "Last name of an employee (max length = 255)",
      "maxLength": 255,
      "type": "string"
    },
    "ExternalLink": {
      "properties": {
        "LinkType": {
          "description": "See External link types",
          "type": "string",
          "enum": ["Facebook", "GooglePlus", "LinkedIn", "Twitter", "Website"]
        },
        "Url": {
          "description": "URL for service e.g. http://twitter.com/xeroapi",
          "type": "string"
        },
        "Description": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "UpdatedDateUTC": {
      "type": "string",
      "readOnly": true,
      "examples": ["/Date(1573755038314)/"]
    },
    "StatusAttributeString": {
      "description": "A string to indicate if a invoice status",
      "type": "string",
      "examples": ["ERROR"]
    },
    "ValidationErrors": {
      "description": "Displays array of validation error messages from the API",
      "type": "array",
      "items": {
        "properties": {
          "Message": {
            "description": "Validation error message",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "type": "object",
  "default": {
    "EmployeeID": "3fa85f64-5717-4562-b3fc-2c963f66afa6",
    "Status": "ACTIVE",
    "FirstName": "string",
    "LastName": "string",
    "ExternalLink": {
      "LinkType": "Facebook",
      "Url": "string",
      "Description": "string"
    },
    "UpdatedDateUTC": "/Date(1573755038314)/",
    "StatusAttributeString": "ERROR",
    "ValidationErrors": [
      {
        "Message": "string"
      }
    ]
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
