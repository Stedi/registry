{
  "type": "object",
  "properties": {
    "metadata": {
      "type": "object",
      "description": "Set of custom key-values specific to the object. The keys are strings and values are arrays of strings. The set of valid keys is always the consignee's list of keys, even if call was made by a different party.\n",
      "examples": [
        {
          "purchase_order": ["12345"],
          "sku": ["abc123"]
        }
      ]
    },
    "_object": {
      "type": "string",
      "description": "Type of the object",
      "examples": ["/shipments"]
    },
    "id": {
      "type": "integer",
      "description": "Unique identifier for the shipment",
      "examples": [2983]
    },
    "name": {
      "type": "string",
      "description": "The name of the shipment",
      "examples": ["ABC Shipment"]
    },
    "transportation_mode": {
      "type": "string",
      "enum": [
        "ocean",
        "air",
        "truck",
        "rail",
        "unknown_transportation",
        "ocean_air",
        "truck_intl",
        "warehouse_storage"
      ],
      "examples": ["ocean"]
    },
    "freight_type": {
      "type": "string",
      "enum": ["port_to_door", "port_to_port", "door_to_door", "door_to_port"],
      "examples": ["port_to_door"]
    },
    "incoterm": {
      "type": "string",
      "enum": ["EXW", "FOB", "FAS", "FCA", "CPT", "CFR", "CIF", "CIP", "DAT", "DAP", "DDP", "DPU"],
      "examples": ["FOB"]
    },
    "calculated_weight": {
      "type": "object",
      "properties": {
        "_object": {
          "type": "string",
          "description": "String representing the object’s type. Always `/quantity/weight` for this object.",
          "examples": ["/quantity/weight"]
        },
        "value": {
          "type": "number",
          "description": "Required. Specifies the weight as a float.",
          "examples": [2300.4]
        },
        "unit": {
          "type": "string",
          "description": "Required. Unit of measurement. \"kg\" for kilograms and \"lbs\" for pounds.",
          "enum": ["kg", "lbs"]
        }
      }
    },
    "calculated_volume": {
      "type": "object",
      "properties": {
        "_object": {
          "type": "string",
          "description": "Type of the object",
          "examples": ["/quantity/volume"]
        },
        "value": {
          "type": "number",
          "description": "total volume",
          "examples": [472.62]
        },
        "unit": {
          "type": "string",
          "description": "unit of measurement. \"cbm\" for cubic meters. \"cbft\" for cubic feet.",
          "enum": ["cbm", "cbft"]
        }
      }
    },
    "pieces": {
      "type": "integer",
      "examples": [8372]
    },
    "it_number": {
      "type": "string",
      "examples": ["V12345678"]
    },
    "created_date": {
      "type": "string",
      "format": "date-time"
    },
    "status": {
      "type": "string",
      "enum": [
        "seller_location",
        "in_transit_to_departure_port",
        "departure_port",
        "in_transit_to_arrival_port",
        "arrival_port",
        "in_transit_to_final_destination",
        "final_destination"
      ],
      "examples": ["in_transit_to_arrival_port"]
    },
    "priority": {
      "type": "string",
      "enum": ["standard", "high"],
      "description": "The level of attention Flexport should give to this shipment",
      "examples": ["standard"]
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "estimated_departure_date": {
      "type": "string",
      "format": "date-time"
    },
    "actual_departure_date": {
      "type": "string",
      "format": "date-time"
    },
    "estimated_arrival_date": {
      "type": "string",
      "format": "date-time"
    },
    "actual_arrival_date": {
      "type": "string",
      "format": "date-time"
    },
    "cargo_ready_date": {
      "type": "string",
      "format": "date"
    },
    "wants_freight_management_bco": {
      "type": "boolean",
      "description": "Determines if a shipment bypasses a freight forwader and is booked directly with a steamship line.",
      "examples": [true]
    },
    "wants_flexport_freight": {
      "type": "boolean",
      "description": "Determines if a shipment is using Flexport services for a combination of import customs clearance or final leg delivery.",
      "examples": [true]
    },
    "estimated_picked_up_in_full_date": {
      "type": "string",
      "format": "date-time"
    },
    "actual_picked_up_in_full_date": {
      "type": "string",
      "format": "date-time"
    },
    "estimated_delivered_in_full_date": {
      "type": "string",
      "format": "date-time"
    },
    "actual_delivered_in_full_date": {
      "type": "string",
      "format": "date-time"
    },
    "wants_export_customs_service": {
      "type": "boolean",
      "description": "Determines if the shipment requires Flexport to provide export customs services.",
      "examples": [true]
    },
    "wants_import_customs_service": {
      "type": "boolean",
      "description": "Determines if the shipment requires Flexport to provide import customs services.",
      "examples": [true]
    },
