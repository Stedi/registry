{
  "type": "object",
  "properties": {
    "_object": {
      "type": "string",
      "description": "Type of the object. Always /document for this object.",
      "example": "/document"
    },
    "id": {
      "type": "string",
      "description": "Unique identifier for the document"
    },
    "file_name": {
      "type": "string"
    },
    "document_type": {
      "type": "string",
      "description": "Type of the document."
    },
    "memo": {
      "type": "string",
      "description": "A short message or description about the document"
    },
    "file_link": {
      "type": "string",
      "format": "uri",
      "description": "The link that can be used to download the file."
    },
    "archived_at": {
      "type": "string",
      "description": "Date string this document was archived. Null if not archived."
    },
    "file_metadata": {
      "type": "object",
      "properties": {
        "_object": {
          "type": "string",
          "description": "Type of the object. Always /file_metadata for this object.",
          "example": "/file_metadata"
        },
        "size": {
          "type": "integer",
          "description": "Size of the file in bytes",
          "example": 20000
        },
        "uploaded_at": {
          "type": "string",
          "description": "Date this document was uploaded."
        }
      }
    },
    "shipment": {
      "type": "object",
      "properties": {
        "_object": {
          "type": "string",
          "example": "/api/refs/object"
        },
        "ref_type": {
          "type": "string",
          "example": "/shipment"
        },
        "link": {
          "type": "string",
          "example": "https://api.flexport.com/shipments/123"
        },
        "id": {
          "type": "integer",
          "example": 123
        }
      }
    }
  },
  "default": {
    "_object": "/document",
    "id": "string",
    "file_name": "string",
    "document_type": "string",
    "memo": "string",
    "file_link": "https://example.com/path",
    "archived_at": "string",
    "file_metadata": {
      "_object": "/file_metadata",
      "size": 20000,
      "uploaded_at": "string"
    },
    "shipment": {
      "_object": "/api/refs/object",
      "ref_type": "/shipment",
      "link": "https://api.flexport.com/shipments/123",
      "id": 123
    }
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}