{
  "title": "ContainerAndCargo",
  "required": ["cargoType"],
  "type": "object",
  "properties": {
    "cargoType": {
      "pattern": "DRY|REEF",
      "type": "string"
    },
    "containerSizeType": {
      "type": "string"
    },
    "dangerousCargo": {
      "type": "boolean"
    },
    "isoContainerCode": {
      "type": "string"
    },
    "kgsPerContainer": {
      "type": "integer",
      "format": "int32"
    },
    "lineitemBasCharge": {
      "type": "string"
    },
    "maerskCommodityCode": {
      "type": "string"
    },
    "mtonPerContainer": {
      "type": "number"
    },
    "quantity": {
      "type": "integer",
      "format": "int32"
    }
  },
  "default": {
    "cargoType": "REEF",
    "containerSizeType": "string",
    "dangerousCargo": true,
    "isoContainerCode": "string",
    "kgsPerContainer": 1,
    "lineitemBasCharge": "string",
    "maerskCommodityCode": "string",
    "mtonPerContainer": 0,
    "quantity": 1
  },
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
