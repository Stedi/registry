      "type": "object",
      "properties": {
        "serviceName": {
          "description": "The main services are\nQuote Request\nRate\nCapacity\nBooking\nTracking\n",
          "type": "string"
        },
        "serviceMethod": {
          "description": "This might be used to indicate the type of transaction (e.g. in the Booking service we have book, cancel and update as the main methods )",
          "enum": ["PLACED", "BOOK", "CANCEL", "UPDATE", "UPDATE_DOCUMENTS", "UPDATE_COST"]
        },
        "serviceComment": {
          "description": "Additional information about the service",
          "type": "object",
