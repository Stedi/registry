      "xml": {
        "name": "MessageHeader"
      },
      "required": ["messageID", "conversationID"],
      "type": "object",
      "properties": {
        "messageID": {
          "description": "This value uniquely identifies each message.\nYou can use this value to locate the particular message on the Exceptions, Queue manager, and logs.\n",
          "type": "string"
        },
        "conversationID": {
          "description": "Useful to inform the system how to process the inbound messages: Sequential or Parallel. If multiple messages have same conversionId, they will be processed sequentially. \n\nUse case: process messages sequentially \n[e.g. 83c732b1-1514-418e-a8ae-a69618ea9d60]\n",
          "type": "string"
        }
      }
