{
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "description": "Type of this carton container",
      "example": "Box or pallet"
    },
    "barcode": {
      "type": "string",
      "description": "Barcode assigned to this carton"
    },
    "measurements": {
      "type": "object",
      "properties": {
        "total_weight_oz": {
          "type": "number",
          "description": "Total weight in ounces",
          "format": "double"
        },
        "length_in": {
          "type": "number",
          "description": "Total length in inches",
          "format": "double"
        },
        "width_in": {
          "type": "number",
          "description": "Total width in inches",
          "format": "double"
        },
        "depth_in": {
          "type": "number",
          "description": "Total depth in inches",
          "format": "double"
        }
      },
      "additionalProperties": false,
      "description": "Measurements of a carton"
    },
    "cartons": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "description": "ID assigned to this carton",
            "format": "int32"
          },
          "type": {
            "type": "string",
            "description": "Type of this carton container",
            "example": "Box or pallet"
          },
          "barcode": {
            "type": "string",
